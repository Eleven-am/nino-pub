<!-- The log out confirm block -->
<div id="log-holder">
    <div id="logout-container">
        <div id="log-message">
            <span>Clicking yes would log you out, do you wish to proceed?</span>
        </div>
        <ul id="logout-list">
            <li id="logout-confirm">yes</li>
            <li id="logout-reject">no</li>
        </ul>
    </div>
</div>

<!-- The random block -->
<div id="recent-container"></div>

<!-- The principal block -->
<div id="primary-block">
    <div id="mov-container" class="basic-container" style="display: none">
        <div class="basic-span"><span>movies</span></div>
        <ul id="movies-list" class="basic-list"></ul>
