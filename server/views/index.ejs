            <div class="information">
                <label id="video-title"></label>
                <div class="spacer"></div>
                <span id="episodeName"></span>
                <div class="spacer"></div>
                <span id="video-deets"></span>
            </div>
        </div>

        <svg id="playFlash" viewBox="0 0 494.148 494.148">
