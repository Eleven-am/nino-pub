                <ul id="upNextInfo">
                    <li id="upNextTitle"></li>
                    <li id="upNextOverview"></li>
                </ul>
            </div>
        </div>

        <div id="sub-window">
            <ul id="sub-list">
                <li id="nan">None</li>
                <div class="sub-divider"></div>
                <li id="eng" data-id="en" class="subs">English</li>
                <div class="sub-divider"></div>
                <li id="fre" data-id="fr" class="subs">French</li>
                <div class="sub-divider"></div>
                <li id="ger" data-id="de" class="subs">German</li>
            </ul>
        </div>

        <div id="download-holder">
            <div id="downloadContainer">
                <div id="downMessage">
                    <span id="downMessage-span">Enter an auth key</span>
                    <input type="text" id="auth-key" placeholder="auth key">
                </div>
                <ul id="down-list">
                    <li id="downAccept">download</li>
                    <li id="downReject">exit</li>
                </ul>
            </div>
        </div>

        <div id="shareHolder">
            <div id="shareContainer">
                <div id="shareMessage">
                    <span id="shareMessage-span">Share from current position ?</span>
                    <input type="text" id="shareOut" readonly/>
                </div>
                <ul id="share-list">
                    <li id="shareAccept">yes</li>
                    <li id="shareReject">no</li>
                </ul>
            </div>
        </div>

    </div>
